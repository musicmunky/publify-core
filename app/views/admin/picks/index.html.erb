<% content_for :page_heading do %>
<h2>
  <i class="glyphicon glyphicon-large glyphicon-duplicate"></i>
  Manage Picks
  <%= link_to("New Pick", {controller: 'picks', action: 'new'}, id: 'dialog-link', class: 'btn btn-info pull-right') %>
</h2>
<% end %>

<table class='table table-striped'>
  <thead>
    <tr class='noborder'>
      <th>Title</th>
      <th>Author</th>
      <th>Type</th>
    </tr>
  </thead>
  <%= render 'picks' %>
</table>
